{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "es5",
        "declaration": false,
        "composite": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": ["src"]
}
